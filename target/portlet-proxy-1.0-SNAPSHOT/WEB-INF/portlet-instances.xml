<!DOCTYPE deployments PUBLIC "-//JBoss Portal//DTD Portlet Instances 2.6//EN"
        "http://www.jboss.org/portlet/dtd/portlet-instances_2_6.dtd">
        
<deployments>

    <deployment>
        <if-exists>overwrite</if-exists>
        <instance>
            <instance-id>DashboardPortletInstance</instance-id>
            <portlet-ref>DashboardPortlet</portlet-ref>
        </instance>
    </deployment>

    <deployment>
        <if-exists>overwrite</if-exists>
        <instance>
            <instance-id>AjaxPortletInstance</instance-id>
            <portlet-ref>AjaxPortlet</portlet-ref>
        </instance>
    </deployment>

</deployments>
